{
  "name": "bryantyao.github.io-dev",
  "version": "1.0.0",
  "description": "Development Version of My GH Pages",
  "private": true,
  "main": "index.html",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build && node scripts/create_cname.js",
    "predeploy": "npm run build",
    "nbdeploy": "node scripts/deploy.js",
    "deploy": "node scripts/deploy.js",
    "deploy:production": "PUBLIC_URL='https://bryantyao.github.io/' NODE_ENV=production npm run deploy",
    "deploy:test": "NODE_ENV=test npm run deploy",
    "view-build": "serve -s build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryantyao/bryantyao.github.io-dev.git"
  },
  "author": "Bryant Yao",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bryantyao/bryantyao.github.io-dev/issues"
  },
  "homepage": "https://dev.bryantyao.com",
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.5",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "popper.js": "^1.11.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-css": "^0.11.1",
    "gh-pages": "^1.0.0",
    "react-scripts": "^1.0.10",
    "serve": "^6.0.6"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ],
      "browserify-css"
    ]
  }
}
